{
  "name": "blogger-pwa",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:types": "tsc --noEmit",
    "generate": "tsx ./scripts/generate.ts",
    "build": "npm run generate && wrangler deploy --env production --dry-run --outdir=./build/workers",
